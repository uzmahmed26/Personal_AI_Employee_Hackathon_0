# Environment variables for Personal AI Employee System

# Gmail API credentials
GOOGLE_APPLICATION_CREDENTIALS=your_google_application_credentials_path
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REFRESH_TOKEN=your_google_refresh_token
GOOGLE_ACCESS_TOKEN=your_google_access_token

# WhatsApp tokens and keys
WHATSAPP_TOKEN=your_whatsapp_token
WHATSAPP_PHONE_NUMBER_ID=your_whatsapp_phone_number_id
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your_whatsapp_webhook_verify_token

# LinkedIn tokens and keys
LINKEDIN_ACCESS_TOKEN=your_linkedin_access_token
LINKEDIN_API_KEY=your_linkedin_api_key
LINKEDIN_SECRET_KEY=your_linkedin_secret_key

# Other API keys and secrets
API_KEY=your_api_key
SECRET_KEY=your_secret_key
DATABASE_URL=your_database_url